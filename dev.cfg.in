# Buildout
# --------
#
# Bring it all together.
[buildout]
extends = etc/base.cfg
develop = .
parts =
    python
    test
    omelette
    paste-dev


# Secrets
# -------
#
# Fill in the password to EDRN's manager account
[secrets]
ldap-manager-password = 


# LabCAS
# ------
#
# Specialized settings for developement
[labcas]
staging = /tmp/labcas/staging
archive = /tmp/labcas/archive
